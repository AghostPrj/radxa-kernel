	rk1000_ctl: rk1000-ctl@40 {
		status = "okay";
		compatible = "rockchip,rk1000-ctl";
		reg = <0x40>;
		reset-gpios = <&gpio1 RK_PC4 GPIO_ACTIVE_LOW>;
		clocks = <&cru SCLK_I2S1_OUT>;
		clock-names = "mclk";
		pinctrl-names = "default";
		pinctrl-0 = <&i2s1_2ch_mclk>;
	};

	rk1000_tve: rk1000-tve@42 {
		status = "okay";
		compatible = "rockchip,rk1000-tve";
		reg = <0x42>;
		rockchip,data-width = <24>;
		rockchip,output = "rgb";
		rockchip,tvemode = <1>; //0 for PAL, 1 for NTSC
		rockchip,ctl = <&rk1000_ctl>;
		#address-cells = <1>;
		#size-cells = <0>;

		tve_in: port@0 {
			reg = <0>;

			tve_in_rgb: endpoint {
				remote-endpoint = <&rgb_out_tve>;
			};
		};
	};

	rk1000_codec: rk1000-codec@60 {
		compatible = "rockchip,rk1000-codec";
		reg = <0x60>;
		#sound-dai-cells = <0>;
		rockchip,ctl = <&rk1000_ctl>;
		//rockchip,spk-en-gpio = <&gpio3 31 GPIO_ACTIVE_LOW>;
		rockchip,pa-en-time-ms = <5000>;
		status = "okay";
	};
};

