			monitor_sec = <5>; 
			sample_res = <10>;
			bat_res_up = <140>;
			bat_res_down = <20>;
			virtual_power = <0>; 
			//full-led-gpios = <&gpio0 RK_PC0 GPIO_ACTIVE_HIGH>;
