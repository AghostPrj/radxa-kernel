&pinctrl {
	headphone {
		hp_det: hp-det {
			rockchip,pins =
				<2 RK_PB4 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	host_5v {
		usb1_en_oc: usb1-en-oc {
			rockchip,pins =
				<4 RK_PD1 RK_FUNC_GPIO &pcfg_pull_up_10ma>;
		};
	};

	vbus_typec {
		usb0_en_oc: usb0-en-oc {
			rockchip,pins =
				<4 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up_10ma>;
		};
	};

	pcie {
		pcie_pwr: pcie-pwr{
			rockchip,pins =
				<4 RK_PD4 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	sdmmc {
		sdmmc_bus1: sdmmc-bus1 {
			rockchip,pins =
				<4 RK_PB0 RK_FUNC_1 &pcfg_pull_up_10ma>;
		};

		sdmmc_bus4: sdmmc-bus4 {
			rockchip,pins =
				<4 RK_PB0 RK_FUNC_1 &pcfg_pull_up_10ma>,
				<4 RK_PB1 RK_FUNC_1 &pcfg_pull_up_10ma>,
				<4 RK_PB2 RK_FUNC_1 &pcfg_pull_up_10ma>,
				<4 RK_PB3 RK_FUNC_1 &pcfg_pull_up_10ma>;
		};

		sdmmc_clk: sdmmc-clk {
			rockchip,pins =
				<4 RK_PB4 RK_FUNC_1 &pcfg_pull_none_10ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			rockchip,pins =
				<4 RK_PB5 RK_FUNC_1 &pcfg_pull_up_10ma>;
		};
	};

	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins =
				<2 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		wifi_wake_host: wifi-wake-host {
			rockchip,pins =
				<0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	wireless-bluetooth {
		uart0_gpios: uart0-gpios {
			rockchip,pins =
				<2 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	hym8563 {
		hym8563_int: hym8563-int {
			rockchip,pins =
				<4 RK_PD6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};

